{
  "name": "Sneaky Lingo",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "public/popup.html",
    "default_icon": "ninja_icon.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["dist/content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ]
}
